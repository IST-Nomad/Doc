ntp server ip 10.129.100.251
ntp server ip 10.129.100.253
event manager applet ASA-OFFLINE
 event tag 1 syslog pattern "100 list boolean or Up->Down"
 trigger
  correlate event 1
 action 1.0 syslog msg "ASA goes DOWN"
 action 1.1 cli command "enable"
 action 1.2 cli command "configure terminal"
 action 1.3 cli command "interface vlan 200"
 action 1.4 cli command "no ip address"
 action 1.5 cli command "shutdown"
 action 1.6 cli command "interface vlan 230"
 action 1.7 cli command "no ip address"
 action 1.8 cli command "shutdown"
 action 1.9 cli command "interface vlan 100"
 action 2.0 cli command "ip address 10.129.100.254 255.255.255.0"
 action 2.1 cli command "no shutdown"
 action 2.2 cli command "interface vlan 130"
 action 2.3 cli command "ip address 10.129.130.254 255.255.255.0"
 action 2.4 cli command "no shutdown"
 action 2.5 cli command "exit"
 action 2.6 cli command "router eigrp 100"
 action 2.7 cli command "no neighbor 10.129.100.251 Vlan200"
 action 2.8 cli command "no neighbor 10.129.100.253 Vlan200"
 action 2.9 cli command "neighbor 10.129.100.251 Vlan100"
 action 3.0 cli command "neighbor 10.129.100.253 Vlan100"
 action 3.1 cli command "end"
 action 3.2 cli command "write memory"
 action 3.3 cli command "clear arp-cache interface vlan 100"
 action 3.4 cli command "clear arp-cache interface vlan 130"
 action 3.5 cli command "clear ip eigrp neighbors"
event manager applet ASA-ONLINE
 event tag 1 syslog pattern "100 list boolean or Down->Up"
 trigger
  correlate event 1
 action 1.0 syslog msg "ASA goes UP"
 action 1.1 cli command "enable"
 action 1.2 cli command "configure terminal"
 action 1.3 cli command "interface vlan 100"
 action 1.4 cli command "no ip address"
 action 1.5 cli command "shutdown"
 action 1.6 cli command "interface vlan 130"
 action 1.7 cli command "no ip address"
 action 1.8 cli command "shutdown"
 action 1.9 cli command "interface vlan 200"
 action 2.0 cli command "ip address 10.129.100.254 255.255.255.0"
 action 2.1 cli command "no shutdown"
 action 2.2 cli command "interface vlan 230"
 action 2.3 cli command "ip address 10.129.130.254 255.255.255.0"
 action 2.4 cli command "no shutdown"
 action 2.5 cli command "exit"
 action 2.6 cli command "router eigrp 100"
 action 2.7 cli command "no neighbor 10.129.100.251 Vlan100"
 action 2.8 cli command "no neighbor 10.129.100.253 Vlan100"
 action 2.9 cli command "neighbor 10.129.100.251 Vlan200"
 action 3.0 cli command "neighbor 10.129.100.253 Vlan200"
 action 3.1 cli command "end"
 action 3.2 cli command "write memory"
 action 3.3 cli command "clear arp-cache interface vlan 200"
 action 3.4 cli command "clear arp-cache interface vlan 230"
 action 3.5 cli command "clear ip eigrp neighbors"
!
end
